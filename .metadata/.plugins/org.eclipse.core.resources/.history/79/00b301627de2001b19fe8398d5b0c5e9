package oopintro;

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Product newProduct = new Product(1, "iPhone 12 Pro", 12500, "The best telephone");
		Product newProduct2 = new Product(2, "iPhone 11", 10500, "The best telephone");
		Product newProduct3 = new Product(3, "iPhone XR", 8500, "The best telephone");

		ProductManager productManager = new ProductManager();
		
		productManager.AddtoCart(newProduct);
		productManager.AddtoCart(newProduct2);
		productManager.AddtoCart(newProduct3);

	}

}
