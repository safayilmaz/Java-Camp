package oopintro;

public class Product {

	int id;
	String productName;
	double unitPrice;
	String detail;
	
	public Product(int id, String productName, double unitPrice, String detail) {
		super();
		this.id = id;
		this.productName = productName;
		this.unitPrice = unitPrice;
		this.detail = detail;
	}
	

	
	
}
