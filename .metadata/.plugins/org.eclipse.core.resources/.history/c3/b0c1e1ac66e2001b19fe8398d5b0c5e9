package oopintro;

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Product newProduct = new Product(1, "iPhone 12 Pro", 12500, "The best telephone");
		newProduct.productName = "iPhone 12 Pro Max";
		newProduct.id = 1;
		newProduct.unitPrice = 12500;
		newProduct.detail = "This is the best telephone";
		
		System.out.print(newProduct.id + " " + newProduct.productName + " " + newProduct.unitPrice + " " + newProduct.detail );
	}

}
